<UserControl x:Class="PracticaUsuarios.Maestro"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PracticaUsuarios"
             mc:Ignorable="d" 
             d:DesignHeight="500" d:DesignWidth="300">
    <StackPanel Background="White">
        <TextBlock FontFamily="Arial" Margin="10,0" Name="lblNombreAlumno">Nombre</TextBlock>
        <TextBox Margin="10,0" Name="txtNombreAlumno"></TextBox>
        <TextBlock FontFamily="Arial" Margin="10,0" Name="lblDireccionAlumno">Dirección</TextBlock>
        <TextBox Margin="10,0" Name="txtDireccionAlumno"></TextBox>
        <TextBlock Margin="10,0" FontFamily="Arial" Name="lblCarreraAlumno">Carrera</TextBlock>
        <ComboBox Margin="10,0" Name="cbCarreraAlumno">
            <ComboBoxItem>Ing. Producción Multimedia</ComboBoxItem>
        </ComboBox>
        <TextBlock FontFamily="Arial" Margin="10,0" Name="lblPreparatoriaAlumno">Preparatoria de procedencia</TextBlock>
        <TextBox Margin="10,0" Name="txtPreparatoriaAlumno"></TextBox>
    </StackPanel>
</UserControl>
