<Window x:Class="PracticaUsuarios.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticaUsuarios"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="300">
    <StackPanel>
        <TextBlock Margin="10,0" FontFamily="Arial">Nuevo Usuario</TextBlock>
        <ComboBox Name="cbUsuario" Margin="10,0" SelectionChanged="cbUsuario_SelectionChanged">
            <ComboBoxItem>Alumno</ComboBoxItem>
            <ComboBoxItem>Maestro</ComboBoxItem>
        </ComboBox>
        <StackPanel Name="spPanelConfiguracion">
            
        </StackPanel>
        <Button Margin="225,10,10,0" Content="Guardar"/>
    </StackPanel>
</Window>
