<UserControl x:Class="PracticaUsuarios.ControlMaestro"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PracticaUsuarios"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="350">
    <StackPanel Background="White">
        <TextBlock Margin="10,10,10,0" Name="lblNombreM" FontWeight="Bold" FontFamily="Arial">Nombre</TextBlock>
        <TextBox Margin="10,0,10,0" Name="txtNombreM"></TextBox>
        <TextBlock Margin="10,10,10,0" Name="lblTelefonoM" FontWeight="Bold" FontFamily="Arial">Teléfono</TextBlock>
        <TextBox Margin="10,0,10,0" Name="txtTelefonoM"></TextBox>
        <TextBlock Margin="10,10,10,0" Name="lblCedulaM" FontWeight="Bold" FontFamily="Arial">Cédula Profesional</TextBlock>
        <TextBox Margin="10,0,10,0" Name="txtCedulaM"></TextBox>
        <TextBlock Margin="10,10,10,0" Name="lblCarreraM" FontFamily="Arial" FontWeight="Bold">Carrera Estudiada</TextBlock>
        <TextBox Margin="10,0,10,0" Name="txtCarreraM"></TextBox>
    </StackPanel>
</UserControl>